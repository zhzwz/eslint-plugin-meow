{
  "name": "eslint-plugin-meow",
  "version": "1.0.12",
  "license": "MIT",
  "repository": "coderzhaoziwei/eslint-plugin-meow",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf dist && unbuild",
    "build:stub": "unbuild --stub",
    "lint": "eslint .",
    "test": "pnpm clear && pnpm i && pnpm run build && pnpm run lint",
    "release": "bump package.json --commit 'chore: release v' --tag 'v' --all",
    "clear": "rimraf dist node_modules"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.3.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-vue": "^8.7.1",
    "eslint-plugin-yml": "^0.14.0",
    "yaml-eslint-parser": "^0.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-meow": "file:",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1",
    "vue": "^3.2.47"
  }
}
